<!DOCTYPE html>
<html>
<head>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta http-equiv="X-UA-Compatible" content="IE=edge" >
  <link rel="stylesheet" href="/static/core/vendor/css/vendor.css">
  <link rel="stylesheet" href="/static/core/css/style.css">
  {{#if module}}
    <link rel="stylesheet" href="/static/{{{module}}}/vendor/css/vendor.css">
    <link rel="stylesheet" href="/static/{{{module}}}/css/style.css">
  {{/if}}
  <script src="/static/core/vendor/js/vendor.js"></script>
  <script src="/static/core/js/script.js"></script>
  <script src="/static/core/js/react.js"></script>
  {{#if module}}
    <script src="/static/{{{module}}}/vendor/js/vendor.js"></script>
    <script src="/static/{{{module}}}/js/script.js"></script>
    <script src="/static/{{{module}}}/js/react.js"></script>
  {{/if}}
  <title>{{componentName}}</title>
  <!-- UDO starts here -->
  <script type="text/javascript">
    var __slf_search_config = {
        api_url: "https://sp10050fd8.guided.ss-omtrdc.net/",
        autocomplete_url: "https://content.atomz.com/autocomplete/sp10/05/0f/d8/",
        sayt_url: "https://sp10050fd8.guided.ss-omtrdc.net/"
    };
    var utag_data={
    "page_canonical_url" : "https://www.sunlife.ca/ca?vgnLocale=en_CA",
    "page_canonical_url_default" : "https://www.sunlife.ca/ca?vgnLocale=en_CA",
    "page_breadcrumb" : "/Home",
    "page_language" : "en",
    "page_category" : "Home",
    "page_subcategory" : ""
    }
  </script>
  <!-- UDO ends here -->
  <!-- Synchronous Tag Support starts here -->
  <script src="//tags.tiqcdn.com/utag/sunlife/ca/dev/utag.sync.js" type="text/javascript"></script>
  <!-- Synchronous Tag Support ends here -->
  <style>
      .node-app-information > .panel-group {
          display: block;
      }
  </style>
</head>
<body>
<!-- Start Tealium -->

    <script type="text/javascript">
        (function(a, b, c, d) {
        a = '//tags.tiqcdn.com/utag/sunlife/ca/dev/utag.js';
        b = document;
        c = 'script';
        d = b.createElement(c);
        d.src = a;
        d.type = 'text/java' + c;
        d.async = true;
        a = b.getElementsByTagName(c)[0];
        a.parentNode.insertBefore(d, a);
        })();
    </script>

<!-- End Tealium -->
<div class="container">
    {{{componentContent}}}
</div>
<div class="container node-app-information">
    <div class="panel-group">
        <div class="panel panel-default">
            <div class="panel-heading">
            <h4 class="panel-title">
                <a data-toggle="collapse" href="#source-code">Source HTML Code</a>
            </h4>
            </div>
            <div id="source-code" class="panel-collapse collapse">
            <div class="panel-body"><pre><code>{{componentContent}}</code></pre></div>            
            </div>
        </div>
        <div class="panel panel-default">
            <div class="panel-heading">
            <h4 class="panel-title">
                <a data-toggle="collapse" href="#component-information">Component Information</a>
            </h4>
            </div>
            <div id="component-information" class="panel-collapse collapse">
            <div class="panel-body"><pre><code>{{{componentInformation}}}</code></pre></div>            
            </div>
        </div>
    </div>
</div>
</body>
</html>