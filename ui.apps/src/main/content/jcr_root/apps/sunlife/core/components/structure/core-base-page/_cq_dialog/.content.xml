<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <htmlid
                                        jcr:primaryType="nt:unstructured"
                                        sling:hideResource="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </basic>
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <section
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Additional SEO Tags"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <canonicalUrl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Canonical URL"
                                                fieldLabel="Canonical URL Overwrite"
                                                name="./canonicalUrl"/>
                                            <advancedPageType
                                                granite:class="cq-dialog-dropdown-showhide"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                name="./advancedPageType">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <regular
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Regular page"
                                                        value="regular"/>
                                                    <cnw
                                                        jcr:primaryType="nt:unstructured"
                                                        text="CNW news page"
                                                        value="cnw"/>
                                                    <advisor
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Advisor page"
                                                        value="advisor"/>
                                                    <article
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Article page"
                                                        value="article"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"/>
                                            </advancedPageType>
                                            <advisorType
                                                granite:class="hide list-option-listfrom-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                name="./advisorType">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <regular
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Regular Advisor"
                                                        value="ADVISOR"/>
                                                    <mac
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Multi Advisor Corporation (MAC)"
                                                        value="CORP"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="advisor"/>
                                            </advisorType>
                                        </items>
                                    </section>
                                    <socialMediaSection
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Additional Social Tags"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Description"
                                                fieldLabel="Description Overwrite"
                                                name="./socialMediaDescripton"/>
                                            <image
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Image"
                                                fieldLabel="Image Overwrite"
                                                name="./socialMediaImage"/>
                                        </items>
                                    </socialMediaSection>
                                    <tools-resources
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title=""
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <headInclude
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="Snippet to include in head section on the page"
                                                fieldLabel="Head Include"
                                                name="./headInclude"/>
                                            <bodyInclude
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="Snippet to include at the end of body section"
                                                fieldLabel="Body Include"
                                                name="./bodyInclude"/>
                                        </items>
                                    </tools-resources>
                                    <error-page
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Error Page Configuration"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <errorPage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Error pages for this content tree"
                                                fieldLabel="Error Pages located path"
                                                name="./errorPages"/>
                                        </items>
                                    </error-page>
									<page-indexing
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Page Indexing"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                             <pageIndexing
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Check if the page should not be displayed in sitemap"
                                                name="./pageIndexing"
                                                text="Skip Search Indexing"
                                                uncheckedValue="false"
                                                value="{Boolean}true"/>
                                        </items>
                                    </page-indexing>
                                    <font-awesome
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Font Awesome"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                             <disablePseudoElementIcon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Check to disable Pseudo-elements to render icons using CSS"
                                                name="./disablePseudoElementIcon"
                                                text="Disable Pseudo-elements Icons"
                                                uncheckedValue="false"
                                                value="{Boolean}true"/>
                                        </items>
                                    </font-awesome>
									<alternate-url
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Alternate URLs"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <alt-langague
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldDescription="Click 'Add' to add a new alternate url">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./alternateUrls">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <alternateLanguage
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Language"
                                                            name="./alternateLanguage"/>
                                                        <alternateUrl
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldLabel="Url"
                                                            name="./alternateUrl"/>
                                                        <defaultLanguage
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                            defaultValue="false"
                                                            fieldDescription="Is default language?"
                                                            fieldLabel="Is default language?"
                                                            name="./defaultLanguage"
                                                            text="Is default language?"
                                                            value="{Boolean}true"/>
                                                    </items>
                                                </field>
                                            </alt-langague>
                                        </items>
                                    </alternate-url>
                                </items>
                            </columns>
                        </items>
                    </general>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
