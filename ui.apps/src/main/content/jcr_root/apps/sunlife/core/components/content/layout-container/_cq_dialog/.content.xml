<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Layout Container"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[sunlife.core.author.layout-container]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container"
        margin="{Boolean}true">
        <items jcr:primaryType="nt:unstructured">
            <columns
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                margin="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <noc
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Number Of Columns"
                                name="./noc">
                                <items jcr:primaryType="nt:unstructured">
                                    <_x0031_
                                        jcr:primaryType="nt:unstructured"
                                        text="1"
                                        value="1"/>
                                    <_x0032_
                                        jcr:primaryType="nt:unstructured"
                                        text="2"
                                        value="2"/>
                                    <_x0033_
                                        jcr:primaryType="nt:unstructured"
                                        text="3"
                                        value="3"/>
                                    <_x0034_
                                        jcr:primaryType="nt:unstructured"
                                        text="4"
                                        value="4"/>
                                    <_x0035_
                                        jcr:primaryType="nt:unstructured"
                                        text="5"
                                        value="5"/>
                                </items>
                            </noc>
                            <type
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Container Type"
                                name="./type">
                                <items jcr:primaryType="nt:unstructured"/>
                            </type>
                            <close-text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Close Button Accessibility Text"
                                name="./closeText"/>
                            <typeVal
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./typeVal"/>
                            <modalID
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Modal ID"
                                name="./modalID"/>
                            <modalTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Modal Title"
                                name="./modalTitle"/>
                            <siteNotificationId
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Site Notification ID"
                                name="./siteNotificationId"/>
                            <titleLevel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Model Title Level"
                                name="./modelTitleLevel">
                                <items jcr:primaryType="nt:unstructured">
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="None"
                                        value="p"/>
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="H2"
                                        value="h2"/>
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="H3"
                                        value="h3"/>
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4"/>
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5"/>
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6"/>
                                </items>
                            </titleLevel>
                            <analyticsID
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        emptyText="Please configure the data-section."
                                        fieldDescription="Please configure the data-section."
                                        fieldLabel="Data section"
                                        name="./analyticsId"/>
                             <flexRequired
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./flexRequired"
                                text="Is flex container?"
                                value="flex-height"/>
                        </items>
                    </column>
                </items>
            </columns>
        </items>
    </content>
</jcr:root>
