<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <section
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Additional SEO Tags"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <canonicalUrl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Canonical URL"
                                                fieldLabel="Canonical URL"
                                                name="./canonicalUrl"/>
                                            <errorPage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                fieldDescription="Error pages for this content tree"
                                                fieldLabel="Error Pages"
                                                name="./errorPages"/>
                                            <advancedPageType
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                name="./advancedPageType">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <regular
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Regular page"
                                                        value="regular"/>
                                                    <cnw
                                                        jcr:primaryType="nt:unstructured"
                                                        text="CNW news page"
                                                        value="cnw"/>
                                                    <advisor
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Advisor page"
                                                        value="advisor"/>
                                                </items>
                                            </advancedPageType>
                                        </items>
                                    </section>
                                    <socialMediaSection
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Additional Social Tags"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Description"
                                                fieldLabel="Description"
                                                name="./socialMediaDescripton"/>
                                            <image
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Image"
                                                fieldLabel="Image"
                                                name="./socialMediaImage"/>
                                        </items>
                                    </socialMediaSection>
                                    <tools-resources
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title=""
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <headInclude
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="Snippet to include in head section on the page"
                                                fieldLabel="Head Include"
                                                name="./headInclude"/>
                                            <bodyInclude
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="Snippet to include at the end of body section"
                                                fieldLabel="Body Include"
                                                name="./bodyInclude"/>
                                        </items>
                                    </tools-resources>
                                </items>
                            </columns>
                        </items>
                    </general>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
