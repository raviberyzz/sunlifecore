<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Advisor Details"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tab
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <tab1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Advisor Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <selectionSection
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Please select"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <advisorType
                                        granite:class="cq-dialog-dropdown-showhide"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        name="./advisorType">
                                        <items jcr:primaryType="nt:unstructured">
                                            <regular
                                                jcr:primaryType="nt:unstructured"
                                                text="Regular Advisor"
                                                value="ADVISOR"/>
                                            <mac
                                                jcr:primaryType="nt:unstructured"
                                                text="Multi Advisor Corporation (MAC)"
                                                value="CORP"/>
                                        </items>
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"/>
                                    </advisorType>
                                </items>
                            </selectionSection>
                            <labelSection
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <address
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Address Label"
                                        fieldLabel="Address Label"
                                        name="./addressLabel"/>
                                    <languages
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Languages Label"
                                        fieldLabel="Languages Label"
                                        name="./languagesLabel"/>
                                    <phone
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Phone Label"
                                        fieldLabel="Phone Label"
                                        name="./phoneLabel"/>
                                    <email
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Email Label"
                                        fieldLabel="Email Label"
                                        name="./emailLabel"/>
                                    <fax
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Fax Label"
                                        fieldLabel="Fax Label"
                                        name="./faxLabel"/>
                                    <disclaimerAsterisk
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Disclaimer Prefix Label"
                                        fieldLabel="Disclaimer Prefix Label"
                                        name="./disclaimerAsteriskLabel"/>
                                    <domain
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Domain"
                                        fieldLabel="Domain"
                                        name="./domain"/>
                                    <advisorLinkLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Advisor Link Label"
                                        fieldLabel="Advisor Link Label"
                                        name="./advisorLinkLabel"/>
                                    <advisorImage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Image"
                                        fieldLabel="Image"
                                        name="./advisorImage"/>
                                    <cellPhone
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Cell Phone Label"
                                        fieldLabel="Cell Phone Label"
                                        name="./cellPhoneLabel"/>
                                </items>
                            </labelSection>
                            <mapSection
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Map Details"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <advisorLocation
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Advisor Location Label"
                                        fieldLabel="Advisor Location Label"
                                        name="./advisorLocationLabel"/>
                                    <viewLargerMap
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="View Larger Map Label"
                                        fieldLabel="View Larger Map Label"
                                        name="./viewLargerMapLabel"/>
                                    <viewLargerMapTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="View Larger Map Title"
                                        fieldLabel="View Larger Map Title"
                                        name="./viewLargerMapTitle"/>
                                    <newWindowImage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Image"
                                        fieldLabel="Image"
                                        name="./newWindowImage"/>
                                </items>
                            </mapSection>
                        </items>
                    </tab1>
                </items>
            </tab>
        </items>
    </content>
</jcr:root>
