<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Card/Content-Block"
          sling:resourceType="cq/gui/components/authoring/dialog"
          helpPath="https://www.adobe.com/go/aem_cmp_list_v2"
          extraClientlibs="[sunlife.core.author.text,sunlife.core.author.card,sunlife.core.author.select-showhide]">
    <content jcr:primaryType="nt:unstructured"
             sling:resourceType="granite/ui/components/coral/foundation/container"
             granite:class="card-type-dialog-container">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured"
                  sling:resourceType="granite/ui/components/coral/foundation/tabs"
                  maximized="{Boolean}true"
                  granite:class="card-type-tabs">
                <items jcr:primaryType="nt:unstructured">
                    <horizontalVertical jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="sunlife/core/components/content/core-card/v1/card/horizontal-vertical" />
                    <avatar jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="sunlife/core/components/content/core-card/v1/card/avatar" />
                    <banner jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="sunlife/core/components/content/core-card/v1/card/banner" />
                    <bannerImage jcr:primaryType="nt:unstructured"
                                 sling:resourceType="granite/ui/components/coral/foundation/include"
                                 path="sunlife/core/components/content/core-card/v1/card/banner-image" />
                    <media jcr:primaryType="nt:unstructured"
                           sling:resourceType="granite/ui/components/coral/foundation/include"
                           path="sunlife/core/components/content/core-card/v1/card/media" />
                    <segmented jcr:primaryType="nt:unstructured"
                               sling:resourceType="granite/ui/components/coral/foundation/include"
                               path="sunlife/core/components/content/core-card/v1/card/segmented" />
                    <statistic jcr:primaryType="nt:unstructured"
                               sling:resourceType="granite/ui/components/coral/foundation/include"
                               path="sunlife/core/components/content/core-card/v1/card/statistic" />
                    <asset jcr:primaryType="nt:unstructured"
                           sling:resourceType="granite/ui/components/coral/foundation/include"
                           path="sunlife/core/components/content/core-card/v1/card/asset" />
                    <cta jcr:primaryType="nt:unstructured"
                         sling:resourceType="granite/ui/components/coral/foundation/include"
                         path="sunlife/core/components/content/core-card/v1/card/cta" />
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>