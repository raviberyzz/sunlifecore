<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="CNW News"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tab
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <tab1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="CNW News"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <generalSection
                                jcr:primaryType="nt:unstructured"
                                jcr:title="General"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <newsArticleUrl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="News Article URL"
                                        name="./newsArticleUrl"
                                        required="{Boolean}true"/>
                                </items>
                            </generalSection>
                            <paginationSection
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Pagination"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <previousText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Previous Button Text"
                                        fieldLabel="Previous Button Text"
                                        name="./previousText"
                                        required="{Boolean}true"/>
                                    <nextText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Next Button Text"
                                        fieldLabel="Next Button Text"
                                        name="./nextText"
                                        required="{Boolean}true"/>
                                    <pageText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Page Label"
                                        fieldLabel="Page Label"
                                        name="./pageText"
                                        required="{Boolean}true"/>
                                    <ofText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Of Label"
                                        fieldLabel="Of Label"
                                        name="./ofText"
                                        required="{Boolean}true"/>
                                </items>
                            </paginationSection>
                        </items>
                    </tab1>
                </items>
            </tab>
        </items>
    </content>
</jcr:root>
