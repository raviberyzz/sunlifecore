<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="CNW News List"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[sunlife.core.author.newslist]">
    <content
        granite:class="cmp-news-list__editor tab-hide-show"
        granite:id="tab-hide-show"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tab
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <configuration
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <selectionSection
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Display Mode"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <newsType
                                        granite:class="newsType"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldDescription="Select 'News Overview' to display a summarized view featuring a selection of news items. Each overview item includes a 'View All Releases' link that directs users to a page with the complete list of news items. Select 'News List' to display a complete selection of news items."
                                        name="./newsType">
                                        <items jcr:primaryType="nt:unstructured">
                                            <overview
                                                granite:class="overview"
                                                jcr:primaryType="nt:unstructured"
                                                text="News Overview"
                                                value="1"/>
                                            <listing
                                                granite:class="listing"
                                                jcr:primaryType="nt:unstructured"
                                                text="News Listing"
                                                value="2"/>
                                        </items>
                                    </newsType>
                                    <newsConfigurations
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                text="News Configurations"/>
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <newsCategories
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                        composite="{Boolean}true"
                                                        fieldDescription="Click 'Add' to add a new news category"
                                                        fieldLabel="News Categories">
                                                        <field
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                            name="./newsCategories">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <newsCategory
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Category"
                                                                    name="./category"
                                                                    required="{Boolean}true"/>
                                                            </items>
                                                        </field>
                                                    </newsCategories>
                                                    <newsArticleUrl
                                                        granite:id="newsOverview-newsArticleUrl"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldDescription="News Article URL"
                                                        fieldLabel="News Article URL"
                                                        name="./newsArticleUrl"
                                                        required="{Boolean}true"/>
                                                </items>
                                            </well>
                                        </items>
                                    </newsConfigurations>
                                </items>
                            </selectionSection>
                        </items>
                    </configuration>
                    <newsOverview
                        jcr:primaryType="nt:unstructured"
                        jcr:title="News Overview"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <generalSectionRelease
                                jcr:primaryType="nt:unstructured"
                                jcr:title="News Overview"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <overview
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <numberOfNews
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Number of news to be displayed"
                                                fieldLabel="Number of news to be displayed"
                                                name="./numberOfNews"/>
                                        </items>
                                    </overview>
                                </items>
                            </generalSectionRelease>
                        </items>
                    </newsOverview>
                    <newsListing
                        jcr:primaryType="nt:unstructured"
                        jcr:title="News Listing"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <generalSectionRelease
                                jcr:primaryType="nt:unstructured"
                                jcr:title="News Listing"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <release
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <latestYear
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Latest Year"
                                                fieldLabel="Latest Year"
                                                name="./latestYear"/>
                                            <numberOfTabs
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Number of tabs"
                                                name="./numberOfTabs">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <option1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="1"
                                                        value="1"/>
                                                    <option2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="2"
                                                        value="2"/>
                                                    <option3
                                                        jcr:primaryType="nt:unstructured"
                                                        text="3"
                                                        value="3"/>
                                                    <option4
                                                        jcr:primaryType="nt:unstructured"
                                                        text="4"
                                                        value="4"/>
                                                    <option5
                                                        jcr:primaryType="nt:unstructured"
                                                        text="5"
                                                        value="5"/>
                                                </items>
                                            </numberOfTabs>
                                            <noNewsMessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Message to be displayed when no news are present"
                                                fieldLabel="Message to be displayed when no news are present"
                                                name="./noNewsMessage"/>
                                        </items>
                                    </release>
                                </items>
                            </generalSectionRelease>
                        </items>
                    </newsListing>
                    <pagination
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Pagination"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <paginationSection
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Pagination"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <previousText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Previous Button Label"
                                        fieldLabel="Previous Button Label"
                                        name="./previousText"/>
                                    <nextText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Next Button Label"
                                        fieldLabel="Next Button Label"
                                        name="./nextText"/>
                                    <pageText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Page Label"
                                        fieldLabel="Page Label"
                                        name="./pageText"/>
                                    <ofText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Of Label"
                                        fieldLabel="Of Label"
                                        name="./ofText"/>
                                    <pageSize
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Page Size"
                                        fieldLabel="Page Size"
                                        name="./pageSize"/>
                                </items>
                            </paginationSection>
                        </items>
                    </pagination>
                    <analytics
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/apps/sunlife/core/components/common/editor/dialog/analytics"/>
                    <spacing
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/apps/sunlife/core/components/common/editor/dialog/spacing"/>
                </items>
            </tab>
        </items>
    </content>
</jcr:root>
