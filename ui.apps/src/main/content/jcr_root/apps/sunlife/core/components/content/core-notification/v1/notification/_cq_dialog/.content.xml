<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Notification"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <notificationType
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="text"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Type"
                                                name="./notificationType">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <information
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Information"
                                                        value="info"/>
                                                    <success
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Success"
                                                        value="success"/>
                                                    <warning
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Warning"
                                                        value="warning"/>
                                                    <error
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Error"
                                                        value="danger"/>
                                                </items>
                                            </notificationType>
                                            <notificationHeading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Notification Heading"
                                                name="./notificationHeading"/>  
                                           <text
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                name="./text"
                                                useFixedInlineToolbar="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                    <button
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Button"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <buttonTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Button Title"
                                                name="./buttonTitle"/>  
                                            <buttonURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="icon"
                                                sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                fieldDescription="Enter the URL for the link. Use a full URL for external links (e.g., 'https://www.example.com'), a relative path for internal links (e.g., '/content/page.html'), or an anchor name prefixed with '#' for in-page navigation (e.g., '#section1'). Ensure the correct format is used for the link to function properly."
                                                fieldLabel="Button URL"
                                                name="./buttonURL"/>
                                             <buttonTarget
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="icon"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Button Target"
                                                name="./buttonTarget">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <sameTab
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Same Tab"
                                                        value="_self"/>
                                                    <newTab
                                                        jcr:primaryType="nt:unstructured"
                                                        text="New Tab"
                                                        value="_blank"/>
                                                </items>
                                            </buttonTarget>
                                            <buttonAccessibilityLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Value of an aria-label attribute for the button. Useful if the visual button text is not very descriptive."
                                                fieldLabel="Button Label"
                                                name="./buttonAccessibilityLabel"
                                                value=""/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </button>
                    	<accessibility
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Accessibility"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <accessibilityLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Value of an aria-label attribute for the notification."
                                                fieldLabel="Label"
                                                name="./accessibilityLabel"
                                                value=""/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </accessibility>
                    <analytics
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/apps/sunlife/core/components/common/editor/dialog/analytics"/>
                     <spacing
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/apps/sunlife/core/components/common/editor/dialog/spacing"/>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>