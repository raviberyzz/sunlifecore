<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Regional Languages Menu"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container"
        margin="{Boolean}true">
        <items jcr:primaryType="nt:unstructured">
            <tab
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <regions
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Regions"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                        <items jcr:primaryType="nt:unstructured">
                                            <regions
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Add (Region links)">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./regions">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <regionName
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Region Name"
                                                            name="regionName"
                                                            required="{Boolean}true"/>
                                                        <regionURL
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldDescription="Enter the URL for the link. Use a full URL for external links (e.g., 'https://www.example.com'), a relative path for internal links (e.g., '/content/page'), or an anchor name prefixed with '#' for in-page navigation (e.g., '#section1'). Ensure the correct format is used for the link to function properly."
                                                            fieldLabel="Region URL"
                                                            name="regionURL"
                                                            required="{Boolean}true"
                                                            suffix=".html"/>
                                                        <regionTarget
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
                                                            fieldLabel="Target"
                                                            name="regionTarget">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <option1
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Same Tab"
                                                                    value="_self"/>
                                                                <option2
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="New Tab"
                                                                    value="_target"/>
                                                            </items>
                                                        </regionTarget>
                                                        <linkHighlight
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                            fieldDescription="Select the Checkbox to set this region as highlight (active)"
                                                            name="./linkHighLight"
                                                            text="Highlight Region "
                                                            value="yes"/>
                                                        <analytics
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            emptyText="Please configure the data title for Analytics."
                                                            fieldDescription="Please configure the data title for Analytics tracking."
                                                            fieldLabel="Data Title"
                                                            name="./dataTitle"/>
                                                        <items
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                            composite="{Boolean}true"
                                                            fieldLabel="(Add) Item links">
                                                            <field
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                name="./items">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <linkName
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldLabel="Item Name"
                                                                        name="linkName"
                                                                        required="{Boolean}true"/>
                                                                    <linkURL
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                        fieldDescription="Enter the URL for the link. Use a full URL for external links (e.g., 'https://www.example.com'), a relative path for internal links (e.g., '/content/page'), or an anchor name prefixed with '#' for in-page navigation (e.g., '#section1'). Ensure the correct format is used for the link to function properly."
                                                                        fieldLabel="Item URL"
                                                                        name="linkURL"
                                                                        required="{Boolean}true"
                                                                        suffix=".html"/>
                                                                    <linkTarget
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
                                                                        fieldLabel="Target"
                                                                        name="linkTarget">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <option1
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Same Tab"
                                                                                value="_self"/>
                                                                            <option2
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="New Tab"
                                                                                value="_target"/>
                                                                        </items>
                                                                    </linkTarget>
                                                                    <analytics
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        emptyText="Please configure the data title for Analytics."
                                                                        fieldDescription="Please configure the data title for Analytics tracking."
                                                                        fieldLabel="Data Title"
                                                                        name="dataTitle"/>
                                                                </items>
                                                            </field>
                                                        </items>
                                                    </items>
                                                </field>
                                            </regions>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </regions>
                    <worldwide
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Worldwide"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                granite:class="testval"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <linkName
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Link Name"
                                                name="./worldwide/linkName"
                                                required="{Boolean}true"/>
                                            <linkURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the URL for the link. Use a full URL for external links (e.g., 'https://www.example.com'), a relative path for internal links (e.g., '/content/page'), or an anchor name prefixed with '#' for in-page navigation (e.g., '#section1'). Ensure the correct format is used for the link to function properly."
                                                fieldLabel="Link URL"
                                                name="./worldwide/linkURL"
                                                required="{Boolean}true"/>
                                            <linkTarget
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Link Target"
                                                name="./worldwide/linkTarget">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <sameTab
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Same Tab"
                                                        value="_self"/>
                                                    <newTab
                                                        jcr:primaryType="nt:unstructured"
                                                        text="New Tab"
                                                        value="_blank"/>
                                                </items>
                                            </linkTarget>
                                            <analytics
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Please configure the data title for Analytics."
                                                fieldDescription="Please configure the data title for Analytics tracking."
                                                fieldLabel="Data Title"
                                                name="./worldwide/dataTitle"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </worldwide>
                    <analytics
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Analytics"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <analytics
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Please configure the data Section for Analytics."
                                                fieldDescription="Please configure the data Section for Analytics tracking."
                                                fieldLabel="Data Section"
                                                name="./dataSection"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </analytics>
                    <spacing
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/apps/sunlife/core/components/common/editor/dialog/spacing"/>
                </items>
            </tab>
        </items>
    </content>
</jcr:root>
