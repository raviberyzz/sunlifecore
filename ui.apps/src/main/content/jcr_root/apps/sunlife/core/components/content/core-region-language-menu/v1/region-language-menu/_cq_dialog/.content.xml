<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Regional Languages Menu"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tab
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <regions
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Regions"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <regions
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Add (Region links)">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./regions">
                                            <items jcr:primaryType="nt:unstructured">
                                                <regionName
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Region Name"
                                                    name="regionName"
                                                    required="{Boolean}true"/>
                                                <regionURL
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Region URL"
                                                    name="regionURL"
                                                    required="{Boolean}true"
                                                    suffix=".html"/>
                                                <regionTarget
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
                                                    fieldLabel="Target"
                                                    name="regionTarget">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <option1
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Same Tab"
                                                            value="_self"/>
                                                        <option2
                                                            jcr:primaryType="nt:unstructured"
                                                            text="New Tab"
                                                            value="_target"/>
                                                    </items>
                                                </regionTarget>
                                                <analytics
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="Please configure the data title for Analytics."
                                                    fieldDescription="Please configure the data title for Analytics tracking."
                                                    fieldLabel="Data Title"
                                                    name="./dataTitle"/>
                                                <items
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldLabel="(Add) Item links">
                                                    <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./items">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <ItemName
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Item Name"
                                                                name="itemName"
                                                                required="{Boolean}true"/>
                                                            <ItemURL
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                fieldLabel="Item URL"
                                                                name="itemURL"
                                                                required="{Boolean}true"
                                                                suffix=".html"/>
                                                            <itemTarget
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
                                                                fieldLabel="Target"
                                                                name="itemTarget">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <option1
                                                                        jcr:primaryType="nt:unstructured"
                                                                        text="Same Tab"
                                                                        value="_self"/>
                                                                    <option2
                                                                        jcr:primaryType="nt:unstructured"
                                                                        text="New Tab"
                                                                        value="_target"/>
                                                                </items>
                                                            </itemTarget>
                                                            <analytics
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                emptyText="Please configure the data title for Analytics."
                                                                fieldDescription="Please configure the data title for Analytics tracking."
                                                                fieldLabel="Data Title"
                                                                name="dataTitle"/>
                                                        </items>
                                                    </field>
                                                </items>
                                                <nextList
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                    checked="{Boolean}false"
                                                    fieldLabel="Do you want to start a new column?"
                                                    name="nextList"
                                                    text="Do you want to start a new column?"
                                                    uncheckedValue="{Boolean}false"
                                                    value="{Boolean}true"/>
                                                <nextListType
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                    name="nextList@TypeHint"
                                                    value="Boolean"/>
                                            </items>
                                        </field>
                                    </regions>
                                </items>
                            </column>
                        </items>
                    </regions>
                    <worldwide
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Worldwide"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <linkName
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Link Name"
                                name="./worldwideName"
                                required="{Boolean}true"/>
                            <linkURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Link URL"
                                name="./worldwideURL"
                                required="{Boolean}true"/>
                            <linkTarget
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Link Target"
                                name="./worldwideTarget">
                                <items jcr:primaryType="nt:unstructured">
                                    <sameTab
                                        jcr:primaryType="nt:unstructured"
                                        text="Same Tab"
                                        value="_self"/>
                                    <newTab
                                        jcr:primaryType="nt:unstructured"
                                        text="New Tab"
                                        value="_blank"/>
                                </items>
                            </linkTarget>
                            <analytics
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                emptyText="Please configure the data title for Analytics."
                                fieldDescription="Please configure the data title for Analytics tracking."
                                fieldLabel="Data Title"
                                name="./worldwideDataTitle"/>
                        </items>
                    </worldwide>
                    <analytics
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Analytics"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <analytics
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Please configure the data Section for Analytics."
                                                fieldDescription="Please configure the data Section for Analytics tracking."
                                                fieldLabel="Data Section"
                                                name="./dataSection"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </analytics>
                    <spacing
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/apps/sunlife/core/components/common/editor/dialog/spacing"/>
                </items>
            </tab>
        </items>
    </content>
</jcr:root>
