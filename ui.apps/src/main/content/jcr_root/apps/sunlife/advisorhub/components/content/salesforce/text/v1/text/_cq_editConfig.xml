<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
    jcr:primaryType="cq:EditConfig">
    <cq:inplaceEditing
        jcr:primaryType="cq:InplaceEditingConfig"
        active="{Boolean}true"
        configPath="inplaceEditingConfig"
        editorType="text">
        <inplaceEditingConfig jcr:primaryType="nt:unstructured">
            <rtePlugins jcr:primaryType="nt:unstructured">
                <styles
                    jcr:primaryType="nt:unstructured"
                    features="*">
                    <styles jcr:primaryType="nt:unstructured">
                        <base
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-body_regular"
                            text="Base"/>
                        <body-size-small
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-body_small"
                            text="Body Size - Small"/>
                        <heading-size-large
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-heading_large"
                            text="Heading Size - Large"/>
                        <heading-size-medium
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-body_medium"
                            text="Heading Size - Medium"/>
                        <heading-size-small
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-body_small"
                            text="Heading Size - Small"/>
                        <title
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-title"
                            text="Title"/>
                        <title-uppercase
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-title_caps"
                            text="Title Uppercase"/>
                        <longform
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-longform"
                            text="Longform"/>
                        <align-left
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-align_left"
                            text="Align Left"/>
                        <align-right
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-align_right"
                            text="Align Right"/>
                        <align-center
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-align_center"
                            text="Align Centre"/>
                        <color-default
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-color_default"
                            text="Colour - Default"/>
                        <color-success
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-color_success"
                            text="Colour - Success"/>
                        <color-weak
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-color_weak"
                            text="Colour - Weak"/>
                        <color-error
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-color_error"
                            text="Colour - Error"/>
                        <color-destructive
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-color_destructive"
                            text="Colour - Destructive Actions"/>
                        <color-inverse
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-color_inverse"
                            text="Colour - Inverse"/>
                        <color-inverse-weak
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-color_inverse-weak"
                            text="Colour - Inverse Weak"/>
                        <color-monospace
                            jcr:primaryType="nt:unstructured"
                            cssName="slds-text-color_monospace"
                            text="Colour - Monospace"/>
                    </styles>
                </styles>
                <htmlRules jcr:primaryType="nt:unstructured">
                    <docType jcr:primaryType="nt:unstructured">
                        <typeConfig
                            jcr:primaryType="nt:unstructured"
                            useSemanticMarkup="{Boolean}true">
                            <semanticMarkupMap
                                jcr:primaryType="nt:unstructured"
                                b="strong"
                                i="em"/>
                        </typeConfig>
                    </docType>
                </htmlRules>
                <tracklinks
                    jcr:primaryType="nt:unstructured"
                    features="*"/>
                <table
                    jcr:primaryType="nt:unstructured"
                    features="-">
                    <hiddenHeaderConfig
                        jcr:primaryType="nt:unstructured"
                        hiddenHeaderClassName="cq-wcm-foundation-aria-visuallyhidden"/>
                </table>
                <paraformat jcr:primaryType="nt:unstructured">
                    <formats jcr:primaryType="nt:unstructured">
                        <default_p
                            jcr:primaryType="nt:unstructured"
                            description="Paragraph"
                            tag="p"/>
                        <default_h1
                            jcr:primaryType="nt:unstructured"
                            description="Heading 1"
                            tag="h1"/>
                        <default_h2
                            jcr:primaryType="nt:unstructured"
                            description="Heading 2"
                            tag="h2"/>
                        <default_h3
                            jcr:primaryType="nt:unstructured"
                            description="Heading 3"
                            tag="h3"/>
                        <default_h4
                            jcr:primaryType="nt:unstructured"
                            description="Heading 4"
                            tag="h4"/>
                        <default_h5
                            jcr:primaryType="nt:unstructured"
                            description="Heading 5"
                            tag="h5"/>
                        <default_h6
                            jcr:primaryType="nt:unstructured"
                            description="Heading 6"
                            tag="h6"/>
                        <default_blockquote
                            jcr:primaryType="nt:unstructured"
                            description="Quote"
                            tag="blockquote"/>
                        <default_hr
                            jcr:primaryType="nt:unstructured"
                            description="Horizontal Rule (visual line break)"
                            tag="hr"/>
                        <default_pre
                            jcr:primaryType="nt:unstructured"
                            description="Preformatted"
                            tag="pre"/>
                    </formats>
                </paraformat>
                <misctools jcr:primaryType="nt:unstructured">
                    <specialCharsConfig jcr:primaryType="nt:unstructured">
                        <chars jcr:primaryType="nt:unstructured">
                            <default_copyright
                                jcr:primaryType="nt:unstructured"
                                entity="&amp;copy;"
                                name="copyright"/>
                            <default_euro
                                jcr:primaryType="nt:unstructured"
                                entity="&amp;euro;"
                                name="euro"/>
                            <default_registered
                                jcr:primaryType="nt:unstructured"
                                entity="&amp;reg;"
                                name="registered"/>
                            <default_trademark
                                jcr:primaryType="nt:unstructured"
                                entity="&amp;trade;"
                                name="trademark"/>
                        </chars>
                    </specialCharsConfig>
                </misctools>
                <links
                    jcr:primaryType="nt:unstructured"
                    features="modifylink,unlink"/>
                <justify
                    jcr:primaryType="nt:unstructured"
                    features="-"/>
                <format
                    jcr:primaryType="nt:unstructured"
                    features="bold,italic"/>
                <sunlife
                    jcr:primaryType="nt:unstructured"
                    features="*"/>
            </rtePlugins>
        </inplaceEditingConfig>
    </cq:inplaceEditing>
</jcr:root>
