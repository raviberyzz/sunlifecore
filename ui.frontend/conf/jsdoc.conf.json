{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"source": {
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": ".+\\index_module.js(doc|x)?$",
		"exclude": [
			"index_module.js",
			"./dist/",
			"./node/",
			"./conf/",
			"./node_modules/",
			"./src/main/webpack/prerequisite/common/external-libs/scripts/easyXDM.js"
		]
	},
	"opts": {
		"destination": "./docs"
	},
	"recurseDepth": 20,
	"plugins": ["plugins/markdown", "jsdoc-mermaid", "plugins/sourcetag"], 
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true
		},
		"path": "ink-docstrap",
		"theme": "cerulean",
		"navType": "vertical",
		"linenums": true,
		"dateFormat": "MMMM Do YYYY, h:mm:ss a"
	}
}
