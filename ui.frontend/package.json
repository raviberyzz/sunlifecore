{
  "name": "aem-maven-archetype",
  "version": "1.0.0",
  "description": "Generates an AEM Frontend project with Webpack",
  "repository": {
    "type": "git",
    "url": ""
  },
  "private": true,
  "main": "src/main/webpack/site/main.ts",
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "dev": "webpack --env dev --config ./webpack.dev.js && npm run build:clientlib",
    "prod": "webpack --config ./webpack.prod.js && clientlib --verbose",
    "start": "webpack-dev-server --open --config ./webpack.dev.js",
    "sync": "aemsync -d -p ../ui.apps/src/main/content",
    "chokidar": "chokidar -c \"clientlib\" ./dist",
    "aemsyncro": "aemsync -w ../ui.apps/src/main/content",
    "aemfed": "aemfed -t \"http://admin:admin@localhost:4504\" -w \"../ui.apps/src/main/content/jcr_root/\"",
    "watch": "npm-run-all --parallel start chokidar aemsyncro",
    "watchsync": "aemfed && watch",
    "build:clientlib": "clientlib --verbose",
    "deploy:local": "aemsync -t http://admin:admin@localhost:4504 -p ../ui.apps/",
    "build-and-deploy": "webpack --env dev --config ./webpack.local.js && npm run build:clientlib",
    "watch:local": "chokidar \"src/main/webpack/**/*.*\" -c \"npm run build-and-deploy\"",
    "local": "npm run watch:local"
  },
  "dependencies": {
    "bootstrap": "^3.3.5",
    "hbs": "^4.0.6",
    "jquery": "^3.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "acorn": "^6.1.0",
    "aem-clientlib-generator": "^1.8.0",
    "aemfed": "^0.1.1",
    "aemsync": "^4.0.1",
    "autoprefixer": "^9.2.1",
    "babel-loader": "^9.1.2",
    "browserslist": "^4.2.1",
    "chokidar-cli": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^10.1.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "cssnano": "^5.0.12",
    "eslint": "^8.4.1",
    "eslint-webpack-plugin": "^3.1.1",
    "glob-import-loader": "^1.2.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.4.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.15",
    "postcss-loader": "^3.0.0",
    "react-datepicker": "^3.1.3",
    "sass": "^1.45.0",
    "sass-loader": "^12.4.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.14.1",
    "terser-webpack-plugin": "^5.2.5",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^4.8.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ]
}
