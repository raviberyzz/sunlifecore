<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/sunlife/settings/wcm/templates/slgi-content-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="sunlife/core/components/structure/base-page"
        advancedPageType="regular"
        advisorType="ADVISOR"
        pageIndexing="false">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <recommended_products_header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/slgi/components/content/recommended-products-article">
                <items jcr:primaryType="nt:unstructured">
                    <item0
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/slgi/english/header/master"
                        selector="pub"/>
                    <item1
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/slgi/english/header/header-advisor"
                        selector="adv"/>
                    <item2
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/slgi/english/header/header-investor"
                        selector="inv"/>
                </items>
            </recommended_products_header>
            <recommended_products_breadcrumb
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/slgi/components/content/recommended-products-article">
                <items jcr:primaryType="nt:unstructured">
                    <item0
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/slgi/english/breadcrumb/master"
                        selector="pub"/>
                    <item1
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/slgi/english/breadcrumb/breadcrumb-advisor"
                        selector="adv"/>
                    <item2
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/slgi/english/breadcrumb/breadcrumb-investor"
                        selector="inv"/>
                </items>
            </recommended_products_breadcrumb>
            <titlebar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/slgi/components/content/titlebar"/>
            <layout_container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/slgi/components/content/layout-container"/>
            <recommended_products_footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/slgi/components/content/recommended-products-article">
                <items jcr:primaryType="nt:unstructured">
                    <item0
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/slgi/english/footer/master"
                        selector="pub"/>
                    <item1
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/slgi/english/footer/footer-advisor"
                        selector="adv"/>
                    <item2
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/slgi/english/footer/footer-investor"
                        selector="inv"/>
                </items>
            </recommended_products_footer>
        </root>
    </jcr:content>
</jcr:root>
