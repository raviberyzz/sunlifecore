<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/sunlife/settings/wcm/templates/slcm-content-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="sunlife/core/components/structure/base-page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <experiencefragment_header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/slcm/components/content/experiencefragment"
                fragmentPath="/content/experience-fragments/sunlife/external/slcm/en/header/master"/>
            <breadcrumb
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/slcm/components/content/breadcrumb"
                hideCurrent="false"
                showHidden="false"
                socialShareReqd="true"
                startLevel="4">
                <socialIcons
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="sunlife/core/components/content/icon-text"
                    title="Follow us"
                    titleLevel="none">
                    <iconsText jcr:primaryType="nt:unstructured">                        
                        <item2
                            jcr:primaryType="nt:unstructured"
                            icon="fa-linkedin-square"
                            link="javascript:void(0)"
                            srText="Share this on Linkedin"
                            target="_self"/>
                    </iconsText>
                </socialIcons>
            </breadcrumb>
            <titlebar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/slcm/components/content/titlebar"/>
            <layout_container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/slcm/components/content/layout-container"/>
            <experiencefragment_footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/slcm/components/content/experiencefragment"
                fragmentPath="/content/experience-fragments/sunlife/external/slcm/en/footer/master"/>
        </root>
    </jcr:content>
</jcr:root>
