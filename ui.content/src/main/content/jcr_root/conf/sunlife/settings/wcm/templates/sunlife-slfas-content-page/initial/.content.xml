<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/sunlife/settings/wcm/templates/sunlife-slfas-content-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="sunlife/legacy/components/structure/base-page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <recommended_products_header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/legacy/components/content/recommended-products-article">
                <items jcr:primaryType="nt:unstructured">
                    <item0
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/ca/slfas/en/header-public/master"
                        selector="public"/>
                    <item1
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/ca/slfas/en/header-secure/master"
                        selector="secure"/>
                    <item2
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/ca/slfas/en/header-slgi/master"
                        selector="slgi"/>
                </items>
            </recommended_products_header>
            <layout_container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/legacy/components/content/legacy-layout-container"/>
            <recommended_products_footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/legacy/components/content/recommended-products-article">
                <items jcr:primaryType="nt:unstructured">
                    <item0
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/ca/slfas/en/footer-public/master"
                        selector="public"/>
                    <item1
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/ca/slfas/en/footer-secure/master"
                        selector="secure"/>
                    <item2
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/ca/slfas/en/footer-slgi/master"
                        selector="slgi"/>
                </items>
            </recommended_products_footer>
        </root>
    </jcr:content>
</jcr:root>
