<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/sunlife/settings/wcm/templates/slgi-dynamic-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="sunlife/core/components/structure/base-page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <recommended_products_header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/slgi/components/content/recommended-products-article">
                <items jcr:primaryType="nt:unstructured">
                    <item0
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/slgi/english/header/master"
                        selector="pub"/>
                    <item1
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/slgi/english/header/header-advisor"
                        selector="adv"/>
                    <item2
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/slgi/english/header/header-investor"
                        selector="inv"/>
                </items>
            </recommended_products_header>
            <recommended_products_breadcrumb
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/slgi/components/content/breadcrumb"
                hideCurrent="false"
                showHidden="false"
                socialShareReqd="true"
                startLevel="4">
                <socialIcons
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="sunlife/core/components/content/icon-text"
                    title="Share this:"
                    titleLevel="none">
                    <iconsText jcr:primaryType="nt:unstructured">
                        <item0
                            jcr:primaryType="nt:unstructured"
                            icon="fa-facebook-square"
                            link="javascript:void(0)"
                            srText="Share this on Facebook"
                            target="_self"/>
                        <item1
                            jcr:primaryType="nt:unstructured"
                            icon="fa-twitter-square"
                            link="javascript:void(0)"
                            srText="Share this on Twitter"
                            target="_self"/>
                        <item2
                            jcr:primaryType="nt:unstructured"
                            icon="fa-linkedin-square"
                            link="javascript:void(0)"
                            srText="Share this on Linkedin"
                            target="_self"/>
                    </iconsText>
                </socialIcons>
            </recommended_products_breadcrumb>
            <titlebar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/slgi/components/content/titlebar"/>
            <collectioncomponent
                jcr:primaryType="nt:unstructured"
                sling:resourceType="contentservices/components/content/collectioncomponent"/>
            <recommended_products_footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/slgi/components/content/recommended-products-article">
                <items jcr:primaryType="nt:unstructured">
                    <item0
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/slgi/english/footer/master"
                        selector="pub"/>
                    <item1
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/slgi/english/footer/footer-advisor"
                        selector="adv"/>
                    <item2
                        jcr:primaryType="nt:unstructured"
                        exfragPath="/content/experience-fragments/sunlife/external/slgi/english/footer/footer-investor"
                        selector="inv"/>
                </items>
            </recommended_products_footer>
        </root>
    </jcr:content>
</jcr:root>
