<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Template"
    allowedPaths="[/content/entities(/.*)?]"
    ranking="{Long}100">
    <jcr:content
        cq:scaffolding="/conf/sunlife/settings/dam/cfm/models/article-model/jcr:content/model"
        cq:templateType="/libs/settings/dam/cfm/model-types/fragment"
        jcr:primaryType="cq:PageContent"
        jcr:title="Article Model"
        sling:resourceSuperType="dam/cfm/models/console/components/data/entity"
        sling:resourceType="dam/cfm/models/console/components/data/entity/default">
        <model
            cq:targetPath="/content/entities"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="wcm/scaffolding/components/scaffolding"
            dataTypesConfig="/mnt/overlay/settings/dam/cfm/models/formbuilderconfig/datatypes"
            maxGeneratedOrder="20">
            <cq:dialog
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/dialog">
                <content
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                    <items
                        jcr:primaryType="nt:unstructured"
                        maxGeneratedOrder="23">
                        <articlePublishedDate
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                            displayedFormat="YYYY-MM-DD HH:mm"
                            emptyText="YYYY-MM-DD HH:mm"
                            fieldDescription="Please provide the Article Published Date"
                            fieldLabel="Article Published Date"
                            listOrder="5"
                            metaType="date"
                            name="articlePublishedDate"
                            renderReadOnly="false"
                            required="on"
                            showEmptyInReadOnly="true"
                            type="datetime"
                            valueFormat="YYYY-MM-DD[T]HH:mm:ss.000Z"
                            valueType="calendar">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                typeHint="Date"/>
                        </articlePublishedDate>
                        <articleHeadline
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Please provide the Article Headline"
                            fieldLabel="Article Headline"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="articleHeadline"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string"/>
                        <articlePageLink
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Please provide the  Article Page URL."
                            fieldLabel="Article Page Link"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="articlePageLink"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string"/>
                        <articleAuthor
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Please provide the Article Author"
                            fieldLabel="Article Author"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="articleAuthor"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string"/>
                        <articleMiniDescription
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Please provide the Article Mini Description"
                            fieldLabel="Article Mini Description"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="articleMiniDescription"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string"/>
                        <articleImage
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/models/editor/components/contentreference"
                            fieldDescription="Please provide the Article Image"
                            fieldLabel="Article Image"
                            filter="hierarchy"
                            listOrder="8"
                            metaType="reference"
                            name="articleImage"
                            nameSuffix="contentReference"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string"/>
                        <articleMainDescription
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/admin/components/authoring/contenteditor/multieditor"
                            cfm-element="Article Main Description"
                            default-mime-type="text/html"
                            fieldDescription="Please provide the Article Main Description"
                            listOrder="2"
                            metaType="text-multi"
                            name="articleMainDescription"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string"/>
                        <articleSocialDescription
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/admin/components/authoring/contenteditor/multieditor"
                            cfm-element="Article Social Description"
                            default-mime-type="text/html"
                            fieldDescription="Please provide the Article Social Description."
                            listOrder="21"
                            metaType="text-multi"
                            name="articleSocialDescription"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string"/>
                        <articleSEODescription
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/admin/components/authoring/contenteditor/multieditor"
                            cfm-element="Article SEO Description"
                            default-mime-type="text/html"
                            fieldDescription="Please provide the Article SEO Description."
                            listOrder="22"
                            metaType="text-multi"
                            name="articleSEODescription"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string"/>
                        <articleKeywords
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Please provide the Article Keywords."
                            fieldLabel="Article Keywords"
                            listOrder="23"
                            maxlength="255"
                            metaType="text-single"
                            name="articleKeywords"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string"/>
                    </items>
                </content>
            </cq:dialog>
        </model>
    </jcr:content>
</jcr:root>
