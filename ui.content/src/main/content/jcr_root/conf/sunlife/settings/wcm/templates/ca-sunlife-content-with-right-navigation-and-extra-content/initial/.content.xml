<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/sunlife/settings/wcm/templates/ca-sunlife-content-with-right-navigation-and-extra-content"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="sunlife/core/components/structure/base-page">
        <root 
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <experiencefragment_header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/ca/components/content/experiencefragment"
                fragmentPath="/content/experience-fragments/sunlife/external/ca/en/header/master"/>
            <breadcrumb  
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/ca/components/content/breadcrumb"/>
            <title  
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/ca/components/content/title"/>
            <container 
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/ca/components/content/container">
                <cq:responsive jcr:primaryType="nt:unstructured">
                    <default
                        jcr:primaryType="nt:unstructured"
                        offset="0"
                        width="9"/>
                </cq:responsive>
            </container>
            <container_1 
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/ca/components/content/container">
                <text
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="sunlife/ca/components/content/text"/>
                <cq:responsive jcr:primaryType="nt:unstructured">
                    <default
                        jcr:primaryType="nt:unstructured"
                        offset="0"
                        width="3"/>
                </cq:responsive>
            </container_1>
            <container_2
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/ca/components/content/container"/>             
            <experiencefragment_footer   
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/ca/components/content/experiencefragment"
                fragmentPath="/content/experience-fragments/sunlife/external/ca/en/footer/master"/>
        </root>
    </jcr:content>
</jcr:root>
