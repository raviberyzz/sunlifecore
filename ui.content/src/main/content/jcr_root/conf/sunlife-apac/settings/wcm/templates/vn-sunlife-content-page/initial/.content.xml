<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/sunlife-apac/settings/wcm/templates/vn-sunlife-content-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="sunlife/core/components/structure/base-page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <experiencefragment_header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/apac/vn/components/content/experiencefragment"
                fragmentPath="/content/experience-fragments/sunlife/external/vn/vn/header/master"/>
            <breadcrumb
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/apac/vn/components/content/breadcrumb"
                hideCurrent="false"
                showHidden="false"
                socialShareReqd="true"
                startLevel="4">
                <socialIcons
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="sunlife/core/components/content/icon-text"
                    title="Chia sáº»:"
                    titleLevel="none">
                    <iconsText jcr:primaryType="nt:unstructured">
                        <item0
                            jcr:primaryType="nt:unstructured"
                            icon="fa-facebook-square"
                            link="javascript:void(0)"
                            srText="Share this on Facebook"                            
                            target="_self"/>
                        <item1
                            jcr:primaryType="nt:unstructured"
                            icon="fa-linkedin-square"
                            link="javascript:void(0)"
                            srText="Share this on Linkedin"
                            target="_self"/>
                         <item2
                            jcr:primaryType="nt:unstructured"
                            icon="vn-icon-email"
                            link="javascript:void(0)"
                            srText="Send mail"
                            target="_self"/>
                    </iconsText>
                </socialIcons>
            </breadcrumb>           
            <titlebar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/apac/vn/components/content/titlebar"/>
            <layout_container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/apac/vn/components/content/layout-container"/>            
            <experiencefragment_footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/apac/vn/components/content/experiencefragment"
                fragmentPath="/content/experience-fragments/sunlife/external/vn/vn/footer/master"/>
        </root>
    </jcr:content>
</jcr:root>
