<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/sunlife-apac/settings/wcm/templates/hk-sunlife-content-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="sunlife/core/components/structure/base-page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <experiencefragment_header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/apac/hk/components/content/experiencefragment"
                fragmentPath="/content/experience-fragments/sunlife/external/hk/en/header/master"/>
            <breadcrumb
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/apac/hk/components/content/breadcrumb"
                hideCurrent="false"
                showHidden="false"
                socialShareReqd="true"
                startLevel="4">
                <socialIcons
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="sunlife/core/components/content/icon-text"
                    title="Share this:"
                    titleLevel="none">
                    <iconsText jcr:primaryType="nt:unstructured">
                        <item0
                            jcr:primaryType="nt:unstructured"
                            icon="fa-facebook-square"
                            target="_self"/>                        
                        <item1
                            jcr:primaryType="nt:unstructured"
                            icon="fa-linkedin-square"
                            target="_self"/>
                    </iconsText>
                </socialIcons>
            </breadcrumb>
            <layout_container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/apac/hk/components/content/layout-container"/>            
            <experiencefragment_footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sunlife/apac/hk/components/content/experiencefragment"
                fragmentPath="/content/experience-fragments/sunlife/external/hk/en/footer/master"/>
        </root>
    </jcr:content>
</jcr:root>
